{
  "domain": "ebay_beta",
  "name": "eBay Beta",
  "config_flow": true,
  "documentation": "https://github.com/theonlyrealcolin/home_assistant_ebay",
  "issues": "https://github.com/theonlyrealcolin/home_assistant_ebay/issues",
  "requirements": [],
  "ssdp": [],
  "version": "0.1.0",
  "zeroconf": [],
  "homekit": {},
  "dependencies": [
    "http",
    "application_credentials"
  ],
  "codeowners": [
    "@theonlyrealcolin"
  ],
  "iot_class": "cloud_polling",
  "auth_providers": [
    {
      "type": "oauth2",
      "description": "eBay OAuth2",
      "client_id": "!secret ebay_beta_client_id",
      "client_secret": "!secret ebay_beta_client_secret"
    }
  ]
}
